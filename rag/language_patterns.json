            "party": [
                "^Entre\\s+les\\s+soussignés\\s*:",
                "^D'une\\s+part\\s*:",
                "^D'autre\\s+part\\s*:",
                "^Entre\\s+:",
                "^D'une\\s+part\\s*,\\s*([^,]+)",
                "^D'autre\\s+part\\s*,\\s*([^,]+)",
                "^Le\\s+client\\s*:",
                "^Le\\s+prestataire\\s*:",
                "^La\\s+société\\s*:",
                "^L'entreprise\\s*:"
            ]
        }
    },
